# ALIAS CONFIG
# adb cmd
alias ainstall="adb install "
alias auninstall="adb uninstall "
# git cmd
alias gadd="git add "
alias gstatus="git status "
alias gcm="git commit "
alias gpush="git push "
alias gpull="git pull "
# other
alias reload_bash="source ~/.bash_profile"




